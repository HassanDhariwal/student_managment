<%= link_to("Back to list", '/student/index', :class => 'back link' ) %>
<div class="students show">
<h2>Show Student</h2>
<table summary="students detail view">
	<tr>
  		<th>ID</th>
  		<td><%= @student.id %></td>
  	</tr>
  	<tr>
		<th>Name</th>
		<td><%= @student.name %></td>    		
	</tr>
	<tr>
		<th>Age</th>
		<td><%= @student.age %></td>
	</tr>
	<tr>
		<th>Father_name</th>
		<td><%= @student.father_name %></td>
	</tr>
	<tr>	
		<th>Phone_number</th>
		<td><%= @student.Phone_number %></td>
	</tr>
	<tr>
		<th>Email</th>
		<td><%= @student.Email %></td>
	</tr>
	<tr>
		<th>Blood_group</th>
		<td><%= @student.Blood_group %></td>
	</tr>
	<tr>
		<th>Created</th>
		<td><%= @student.Created_at %></td>
	</tr>
	<tr>
		<th>Updated</th>
		<td><%= @student.Updated_at %></td>
	</tr>
</table>
</div>