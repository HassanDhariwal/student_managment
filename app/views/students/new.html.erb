
<!-- <= form_with url: "/students", method: :post, local: true do |f| %> -->
<%= form_with :url => students_path, :method => 'post', local: true do |f| %>
  <!-- <= form_with(model: @student,url: "/students", method: :post, local: true) do |f| %> -->

   <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>  
  <p>
    <%= f.label :age %><br>
    <%= f.number_field :age %>
  </p>
  <p>
    <%= f.label :father_name %><br>
    <%= f.text_field :father_name %>
  </p>
  <p>
    <%= f.label :phone_number %><br>
    <%= f.text_field :phone_number %>
  </p>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label :blood_group %><br>
    <%= f.text_field :blood_group %>
  </p>
  <p>
    <%= f.label :section_name %><br>
    <%= f.text_field :section_name %>
  </p>
  <p>
    <%= f.label :teacher_name %><br>
    <%= f.text_field :teacher_name %>
  </p>
  <p>
    <%= f.label :student_range %><br>
    <%= f.number_field :student_range %>
  </p>
  <p>
    <%= f.label :grade %><br>
    <%= f.number_field :grade %>
  </p>
  
  <div class="form-button">
    <%= f.submit ('create student') %>
  </div>
<% end %>
<%= link_to("Back to list", '/students/index', :class => 'back link' ) %>
